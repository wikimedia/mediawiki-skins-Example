{
	"name": "Kagaga-skin",
	"version": "1.0.0",
	"author": [
		"Masayuki Kobayashi"
	],
	"url": "https://kagaga.jp",
	"descriptionmsg": "kagaga-skin-desc",
	"namemsg": "kagaga-specialversion-name",
	"license-name": "MIT",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.38.0"
	},
	"ValidSkinNames": {
		"kagaga-skin": {
			"class": "SkinMustache",
			"args": [
				{
					"name": "kagaga-skin",
					"responsive": true,
					"messages": [
						"kagaga-skin-categories",
						"sitetitle",
						"search",
						"otherlanguages",
						"tagline",
						"navigation-heading"
					],
					"styles": [
						"mediawiki.ui.icon",
						"mediawiki.ui.button",
						"skins.kagaga-skin.styles"
					],
					"scripts": [
						"skins.kagaga-skin"
					]
				}
			]
		}
	},
	"MessagesDirs": {
		"Kagaga-skin": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.kagaga-skin.styles": {
			"class": "ResourceLoaderSkinModule",
			"features": {
				"normalize": true,
				"elements": true,
				"content-tables": true,
				"content-links": false,
				"content-media": false,
				"content-links-external": false,
				"interface-message-box": true,
				"interface-category": false,
				"toc": true
			},
			"targets": [
				"desktop",
				"mobile"
			],
			"styles": [
				"resources/skin.less"
			]
		},
		"skins.kagaga-skin": {
			"targets": [
				"desktop",
				"mobile"
			],
			"packageFiles": [
				"resources/skin.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Kagaga-skin"
	},
	"ResourceModuleSkinStyles": {
		"kagaga-skin": {
			"+ext.echo.styles.badge": "resources/extensions/echo.styles.badge.less"
		}
	},
	"manifest_version": 2
}
